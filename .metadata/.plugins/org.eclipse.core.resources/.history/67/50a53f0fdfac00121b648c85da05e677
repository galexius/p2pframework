package com.example.firstapp;

public class Node {
	public float x;
	public float y;
	private boolean occupied = false;
	
	public Node(){};
	
    public Node(float x, float y) {
		this.x = x;
		this.y = y;
	}


	@Override
    public String toString() {
        return x + ", " + y;
    }


	public boolean isOccupied() {
		return occupied;
	}


	public void setOccupied(boolean occupied) {
		this.occupied = occupied;
	}
}
