package com.example.firstapp;

import android.app.Service;
import android.content.Intent;
import android.graphics.Point;
import android.os.IBinder;

import com.example.bus.MainService;

public class ServiceWrapper extends MainService {
	
	private MainApplication app;

	public ServiceWrapper(MainApplication app){
		this.app = app;		
	}

	@Override
	protected String getHostChannelName() {
		return app.getHostChannelName();
	}

	@Override
	protected String getChannelName() {
		return app.getChannelName();
	}

	@Override
	protected Point getPoint() {
		return app.getPoint();
	}

	@Override
	protected void deleteObserver(MainService mainService) {
		app.addObserver(mainService);
		
	}

	@Override
	protected void addObserver(MainService mainService) {
		// TODO Auto-generated method stub
		
	}

		

}
