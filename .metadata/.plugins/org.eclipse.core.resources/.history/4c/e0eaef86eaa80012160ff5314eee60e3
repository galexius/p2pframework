package com.example.prototypedesktop;

import org.alljoyn.bus.BusAttachment;
import org.alljoyn.bus.ProxyBusObject;
import org.alljoyn.bus.Status;

import com.example.bus.MyInterface;

public class DesktopService {
	
	public void init(){
        /* Create a bus connection and connect to the bus */
        BusAttachment bus = new BusAttachment("com.example.firstapp.prototype");
        Status status = bus.connect();
        if (Status.OK != status) {
            System.out.println("BusAttachment.connect() failed with " + status.toString());
            return;
        }

        /* Get a remote object */
        Class[] ifaces = { MyInterface.class };
        ProxyBusObject proxyObj = bus.getProxyBusObject("com.example.firstapp.prototype",
                                                         "/testobject",
                                                         0, // zero = sessions are not being used
                                                         ifaces);
        MyInterface proxy = proxyObj.getInterface(MyInterface.class);
     
	}

}
